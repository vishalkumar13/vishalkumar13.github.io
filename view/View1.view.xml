<mvc:View controllerName="sap.ui.demoVaccinator.controller.View1" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m" xmlns:core="sap.ui.core">
	<App>
		<pages>
			<Page title="{i18n>covidslot}">
				<content>
					<VBox class="sapUiSizeCompact">
						<HBox justifyContent="SpaceAround">
							<HBox width="50%" justifyContent="Center">
								<VBox renderType="Bare" justifyContent="Center" width="25%">
									<Label id="SystemType" text="{i18n>Pincode}" tooltip="{i18n>Pincode}" labelFor="sourceClient" class="PaddingRight1rem"/>
								</VBox>
								<VBox renderType="Bare" justifyContent="Start">
									<Input></Input>
								</VBox>
							</HBox>
						</HBox>
						<HBox justifyContent="SpaceAround">
							<HBox width="50%" justifyContent="Center">
								<VBox renderType="Bare" justifyContent="Center" width="25%">
									<Label id="Categories" text="{i18n>Date}" tooltip="{i18n>Date}" labelFor="idCategory" class="PaddingRight1rem"/>
								</VBox>
								<VBox renderType="Bare" justifyContent="Start">
									<Input></Input>
								</VBox>
							</HBox>
						</HBox>
						<HBox justifyContent="SpaceAround">
							<HBox width="50%" justifyContent="Center">
								<VBox renderType="Bare" justifyContent="Center" width="25%">
									<Label text="{i18n>CenterName}" tooltip="{i18n>CenterName}" labelFor="FilterName" class="PaddingRight1rem"/>
								</VBox>
								<VBox renderType="Bare" justifyContent="Start">
									<DatePicker></DatePicker>
								</VBox>
							</HBox>
						</HBox>
					</VBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>